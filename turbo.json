{
    "$schema": "https://turbo.build/schema.json",
    "pipeline": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                "dist/**",
                ".next/**"
            ]
        },
        "lint": {
            "outputs": []
        },
        "dev": {
            "dependsOn": [
                "noqte-ui-docs#noqte:dev"
            ],
            "cache": false,
            "persistent": true
        },
        "noqte-ui-docs#noqte:dev": {},
        "noqte-ui-docs#dev": {}
    }
}
